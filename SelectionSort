using namespace std;

class SelectionSort: public Sorter
{
    public:
        SelectionSort();
        ~SelectionSort();
        vector<int>  sort();
        SelectionSort(vector <int> );
        vector<int> reverse();
};

#endif // SELECTIONSORT_H
